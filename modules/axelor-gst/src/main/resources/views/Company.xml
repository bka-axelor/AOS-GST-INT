<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <form name="company-form" title="Company" model="com.axelor.apps.base.db.Company"
    extension="true" id="gst-company-form">
    <extend target="//field[@name='parent']">
      <insert position="after">
        <field name="gstin" if-module="axelor-gst" if="__config__.app.isApp('axelor-gst')"/>
      </insert>
    </extend>
  </form>



</object-views>