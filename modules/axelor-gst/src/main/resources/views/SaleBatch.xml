<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">

  <form onLoad="action-group-supplychain-salebatch-onload"
    onNew="action-group-supplychain-salebatch-onnew" name="sale-batch-form" title="Sale Batch"
    model="com.axelor.apps.sale.db.SaleBatch" width="large" extension="true"
    id="gst-sale-batch-form">


    <extend target="//button[@name='invoicingBatchBtn']">
      <insert position="after">
        <button name="salesOderBatchBtn" title="Sales Order Batch" onClick="save,action-sale-batch-method-action-sale-order-batch"/>
      </insert>
    </extend>
  </form>
  
  <action-method name="action-sale-batch-method-action-sale-order-batch">
  	<call class="com.axelor.apps.gst.web.GstBatchController" method="actionBatceSale"/>
  </action-method>
</object-views>